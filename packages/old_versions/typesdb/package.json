{
  "name": "@vidalii/typesdb",
  "version": "1.0.0",
  "main": "src",
  "author": "Roy Alcala Ortiz",
  "license": "MIT",
  "scripts": {
    "nodemon": "nodemon --exec babel-node src",
    "node": "babel-node src",
    "pm2:start": "pm2 start ecosystem.config.js",
    "pm2:stop:all": "pm2 stop all",
    "pm2:startCLI": "pm2 start src",
    "kill": "sudo kill -9 $(sudo lsof -t -i:4000) | exit 0",
    "server:testNO": "nodemon --exec babel-node jest/serverForClient",
    "gqlApollo:test": "nodemon --exec babel-node jest/testGqlApollo",
    "isolate:test": "nodemon --exec babel-node jest/isolateTest",
    "test": "jest jest",
    "test:watch": "yarn test -- --watch",
    "babel:out:dir": "npx babel src --out-dir lib",
    "babel:out:dir:onefile": "npx babel src --out-file src-onefile.js",
    "babel:out:file": "npx babel src --out-file script-compiled.js"
  },
  "devDependencies": {
    "@babel/cli": "^7.6.4",
    "@babel/core": "^7.8.4",
    "@babel/node": "^7.8.4",
    "@babel/plugin-syntax-dynamic-import": "^7.2.0",
    "@babel/preset-env": "^7.6.3",
    "babel-plugin-ramda": "^2.0.0"
  },
  "dependencies": {
    "apollo-server-fastify": "^2.9.16",
    "axios": "^0.19.2",
    "dotenv": "^8.2.0",
    "fastify": "^2.11.0",
    "fastify-cors": "^3.0.1",
    "fs-extra": "^8.1.0",
    "glob": "^7.1.6",
    "graphql-type-json": "^0.3.1",
    "jsonwebtoken": "^8.5.1",
    "ramda": "^0.26.1",
    "sqlite3": "^4.1.1",
    "typeorm": "^0.2.22",
    "uuid": "^3.4.0",
    "yup": "^0.28.1"
  }
}
