{
    "name": "default",
    "type": "sqlite",
    "database": "packages/db/mydb.sqlite",
    "synchronize": true,
    "logging": false,
    "entities": [
        "src/entities/*.js"
    ],
    "migrations": [
        "src/migration/**/*.ts"
    ],
    "subscribers": [
        "src/subscriber/**/*.ts"
    ]
}