{
    "name": "default",
    "type": "sqlite",
    "database": "packages/db/mydb.sqlite",
    "synchronize": true,
    "logging": false,
    "entities": [
        "src/orm/models/*.js"
    ],
    "migrations": [
        "src/orm/migration/**/*.ts"
    ],
    "subscribers": [
        "src/orm/subscriber/**/*.ts"
    ]
}