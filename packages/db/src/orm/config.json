{
    "name": "default",
    "type": "sqlite",
    "database": "databases/db.sqlite",
    "synchronize": true,
    "logging": false,
    "entities": [
        "src/orm/models/*.js"
    ],
    "migrations": [
        "src/orm/migration/*.js"
    ],
    "subscribers": [
        "src/orm/subscriber/*.js"
    ]
}